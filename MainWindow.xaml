<Window x:Class="Lab3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox Name="txtLibraryPath" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" VerticalAlignment="Top" Width="500"/>
        <Button Name="btnLoadLibrary" Content="Load Library" HorizontalAlignment="Left" Margin="520,10,0,0" VerticalAlignment="Top" Width="75" Click="btnLoadLibrary_Click"/>
        <ListBox Name="listBoxClasses" HorizontalAlignment="Left" Height="350" Margin="10,50,0,0" VerticalAlignment="Top" Width="200" SelectionChanged="listBoxClasses_SelectionChanged"/>
        <StackPanel Name="panelMethods" HorizontalAlignment="Left" Height="350" Margin="220,50,0,0" VerticalAlignment="Top" Width="550"/>
    </Grid>
</Window>
